{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile-railway"
  },
  "deploy": {
    "healthcheckPath": "/health",
    "healthcheckTimeout": 10,
    "numReplicas": 1,
    "restartPolicyMaxRetries": 0,
    "restartPolicyType": "ON_FAILURE"
  }
}
